<%= simple_form_for [current_user, @garage] do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :street_number %>
  <%= f.input :street_address %>
  <%= f.input :city %>
  <%= f.input :country %>
  <%= f.input :capacity_reg_bikes %>
  <%= f.input :capacity_large_bikes %>
  <%= f.input :price_regular_bike %>
  <%= f.input :price_large_bike %>
  <%= f.input :profile_image %>
  <%= f.input :profile_image_cache, as: :hidden %>
  <%= f.button :submit %>
<% end %>
